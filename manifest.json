{
    "manifest_version": 3,
    "name": "Simple Google Form Autofiller",
    "version": "1.0",
    "description": "Autofills text fields in Google Forms",
    "permissions": ["storage", "tabs"],
    "content_scripts": [
        {
            "matches": ["https://docs.google.com/forms/*"],
            "js": ["content.js"]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    }
}
