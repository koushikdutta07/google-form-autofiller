{
    "manifest_version": 3,
    "name": "Google Form Autofiller",
    "version": "2.0",
    "description": "Automatically detects Google Form fields and autofills them dynamically",
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [
        {
            "matches": ["https://docs.google.com/forms/*"],
            "js": ["content.js"]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    }
}
